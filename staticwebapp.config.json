{
  "buildInfo": {
    "buildCommand": "cd frontend && rm -rf node_modules package-lock.json && npm install && npm run build",
    "outputLocation": "frontend/dist",
    "appLocation": "frontend"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }

  ],
  "mimeTypes": {
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".png": "image/png",
    ".gif": "image/gif"
  }

}
